[PHP]

{{#engine}}engine = {{engine}}{{/engine}}

[PHP-language]

; http://php.net/short-open-tag
short_open_tag = {{#short_open_tag}}On{{/short_open_tag}}{{^short_open_tag}}Off{{/short_open_tag}}

; http://php.net/asp-tags
asp_tags = {{#asp_tags}}On{{/asp_tags}}{{^asp_tags}}Off{{/asp_tags}}

; http://php.net/precision
precision = {{#precision}}{{precision}}{{/precision}}{{^precision}}14{{/precision}}

; http://php.net/y2k-compliance
y2k_compliance = {{#y2k_compliance}}On{{/y2k_compliance}}{{^y2k_compliance}}Off{{/y2k_compliance}}

; http://php.net/output-buffering
output_buffering = {{#output_buffering}}{{output_buffering}}{{/output_buffering}}{{^output_buffering}}4096{{/output_buffering}}

{{#zlib_output_compression}}
; http://php.net/zlib.output-compression
zlib.output_compression = On

; http://php.net/zlib.output-compression-level
zlib.output_compression_level = {{#zlib_output_compression_level}}{{zlib_output_compression_level}}{{/zlib_output_compression_level}}{{^zlib_output_compression_level}}-1{{/zlib_output_compression_level}}
{{/zlib_output_compression}}
{{^zlib_output_compression}}
; http://php.net/zlib.output-handler
{{#zlib_output_handler}}
zlib.output_handler = {{zlib_output_handler}}
{{/zlib_output_handler}}
{{/zlib_output_compression}}

; Implicit flush tells PHP to tell the output layer to flush itself
; automatically after every output block.
implicit_flush = {{#implicit_flush}}On{{/implicit_flush}}{{^implicit_flush}}Off{{/implicit_flush}}

; When floats & doubles are serialized store serialize_precision significant
; digits after the floating point. The default value ensures that when floats
; are decoded with unserialize, the data will remain the same.
serialize_precision = {{#serialize_precision}}{{serialize_precision}}{{/serialize_precision}}{{^serialize_precision}}17{{/serialize_precision}}

; http://php.net/allow-call-time-pass-reference
allow_call_time_pass_reference = {{#allow_call_time_pass_reference}}On{{/allow_call_time_pass_reference}}{{^allow_call_time_pass_reference}}Off{{/allow_call_time_pass_reference}}

; http://php.net/safe-mode
safe_mode = {{#safe_mode}}On{{/safe_mode}}{{^safe_mode}}Off{{/safe_mode}}

; http://php.net/expose-php
expose_php = {{#expose_php}}On{{/expose_php}}{{^expose_php}}Off{{/expose_php}}

; http://php.net/max-execution-time
max_execution_time = {{#max_execution_time}}{{max_execution_time}}{{/max_execution_time}}{{^max_execution_time}}30{{/max_execution_time}}

; http://php.net/max-input-time
max_input_time = {{#max_input_time}}{{max_input_time}}{{/max_input_time}}{{^max_input_time}}60{{/max_input_time}}

; http://php.net/memory-limit
memory_limit = {{#memory_limit}}{{memory_limit}}{{/memory_limit}}{{^memory_limit}}128M{{/memory_limit}}

; http://php.net/error-reporting
error_reporting = E_ALL | E_STRICT

; http://php.net/display-errors
display_errors = {{#display_errors}}On{{/display_errors}}{{^display_errors}}Off{{/display_errors}}

; http://php.net/display-startup-errors
display_startup_errors = {{#display_startup_errors}}On{{/display_startup_errors}}{{^display_startup_errors}}Off{{/display_startup_errors}}

; http://php.net/log-errors
log_errors = {{#log_errors}}On{{/log_errors}}{{^log_errors}}Off{{/log_errors}}

; http://php.net/log-errors-max-len
log_errors_max_len = {{#log_errors_max_len}}{{log_errors_max_len}}{{/log_errors_max_len}}{{^log_errors_max_len}}1024{{/log_errors_max_len}}

; http://php.net/ignore-repeated-errors
ignore_repeated_errors = {{#ignore_repeated_errors}}On{{/ignore_repeated_errors}}{{^ignore_repeated_errors}}Off{{/ignore_repeated_errors}}

; http://php.net/ignore-repeated-source
ignore_repeated_source = {{#ignore_repeated_source}}On{{/ignore_repeated_source}}{{^ignore_repeated_source}}Off{{/ignore_repeated_source}}

; http://php.net/report-memleaks
report_memleaks = {{#report_memleaks}}On{{/report_memleaks}}{{^report_memleaks}}Off{{/report_memleaks}}

; http://php.net/track-errors
track_errors = {{#track_errors}}On{{/track_errors}}{{^track_errors}}Off{{/track_errors}}

; http://php.net/html-errors
html_errors = {{#html_errors}}On{{/html_errors}}{{^html_errors}}Off{{/html_errors}}

; http://php.net/error-prepend-string
error_prepend_string = "{{#error_prepend_string}}{{error_prepend_string}}{{/error_prepend_string}}{{^error_prepend_string}}<span style='color: #ff0000'><pre>{{/error_prepend_string}}"

; http://php.net/error-append-string
error_append_string = "{{#error_append_string}}{{error_append_string}}{{/error_append_string}}{{^error_append_string}}</pre></span>{{/error_append_string}}"

; http://php.net/variables-order
variables_order = "GPCS"

; http://php.net/request-order
request_order = "GP"

; http://php.net/register-globals
register_globals = {{#register_globals}}On{{/register_globals}}{{^register_globals}}Off{{/register_globals}}

; http://php.net/register-long-arrays
register_long_arrays = {{#register_long_arrays}}On{{/register_long_arrays}}{{^register_long_arrays}}Off{{/register_long_arrays}}

; http://php.net/register-argc-argv
register_argc_argv = {{#register_argc_argv}}On{{/register_argc_argv}}{{^register_argc_argv}}Off{{/register_argc_argv}}

; http://php.net/auto-globals-jit
auto_globals_jit = {{#auto_globals_jit}}On{{/auto_globals_jit}}{{^auto_globals_jit}}Off{{/auto_globals_jit}}

; http://php.net/post-max-size
post_max_size = {{#post_max_size}}{{post_max_size}}{{/post_max_size}}{{^post_max_size}}128M{{/post_max_size}}

; http://php.net/magic-quotes-gpc
magic_quotes_gpc = {{#magic_quotes_gpc}}On{{/magic_quotes_gpc}}{{^magic_quotes_gpc}}Off{{/magic_quotes_gpc}}

; http://php.net/magic-quotes-runtime
magic_quotes_runtime = {{#magic_quotes_runtime}}On{{/magic_quotes_runtime}}{{^magic_quotes_runtime}}Off{{/magic_quotes_runtime}}

; http://php.net/magic-quotes-sybase
magic_quotes_sybase = {{#magic_quotes_sybase}}On{{/magic_quotes_sybase}}{{^magic_quotes_sybase}}Off{{/magic_quotes_sybase}}

; http://php.net/default-mimetype
default_mimetype = "{{#default_mimetype}}{{default_mimetype}}{{/default_mimetype}}{{^default_mimetype}}text/html{{/default_mimetype}}"

; http://php.net/include-path
include_path = ".{{#include_path}}:{{.}}{{#include_path}}{{^include_path}}:/php/includes{{/include_path}}"

; http://php.net/enable-dl
enable_dl = {{#enable_dl}}On{{/enable_dl}}{{^enable_dl}}Off{{/enable_dl}}

; http://php.net/cgi.force-redirect
; The documentation recommended that this stays on
;cgi.force_redirect = 1

; http://php.net/file-uploads
file_uploads = {{#file_uploads}}On{{/file_uploads}}{{^file_uploads}}Off{{/file_uploads}}

; http://php.net/upload-max-filesize
upload_max_filesize = {{#upload_max_filesize}}{{upload_max_filesize}}{{/upload_max_filesize}}{{^upload_max_filesize}}128M{{/upload_max_filesize}}

max_file_uploads = {{#max_file_uploads}}{{max_file_uploads}}{{/max_file_uploads}}{{^max_file_uploads}}20{{/max_file_uploads}}

; http://php.net/allow-url-fopen
allow_url_fopen = {{#allow_url_fopen}}On{{/allow_url_fopen}}{{^allow_url_fopen}}Off{{/allow_url_fopen}}

; http://php.net/allow-url-include
allow_url_include = {{#allow_url_include}}On{{/allow_url_include}}{{^allow_url_include}}Off{{/allow_url_include}}

; http://php.net/default-socket-timeout
default_socket_timeout = {{#default_socket_timeout}}{{default_socket_timeout}}{{/default_socket_timeout}}{{^default_socket_timeout}}60{{/default_socket_timeout}}

; http://php.net/auto-detect-line-endings
auto_detect_line_endings = {{#auto_detect_line_endings}}On{{/auto_detect_line_endings}}{{^auto_detect_line_endings}}Off{{/auto_detect_line_endings}}

[Date]
; http://php.net/date.timezone
date.timezone = {{#date_timezone}}{{date_timezone}}{{/date_timezone}}{{^date_timezone}}America/Denver{{/date_timezone}} 

[Pdo_mysql]
; http://php.net/pdo_mysql.cache_size
pdo_mysql.cache_size = {{#pdo_mysql_cache_size}}{{pdo_mysql_cache_size}}{{/pdo_mysql_cache_size}}{{^pdo_mysql_cache_size}}2000{{/pdo_mysql_cache_size}} 

[Syslog]
; http://php.net/define-syslog-variables
define_syslog_variables  = {{#define_syslog_variables}}On{{/define_syslog_variables}}{{^define_syslog_variables}}Off{{/define_syslog_variables}}

; Add X-PHP-Originating-Script: that will include uid of the script followed by the filename
mail.add_x_header = {{#mail_add_x_header}}On{{/mail_add_x_header}}{{^mail_add_x_header}}Off{{/mail_add_x_header}}

[SQL]
; http://php.net/sql.safe-mode
sql.safe_mode = {{#sql_safe_mode}}On{{/sql_safe_mode}}{{^sql_safe_mode}}Off{{/sql_safe_mode}}

[ODBC]
; http://php.net/odbc.allow-persistent
odbc.allow_persistent = {{#odbc_allow_persistent}}On{{/odbc_allow_persistent}}{{^odbc_allow_persistent}}Off{{/odbc_allow_persistent}}

; http://php.net/odbc.check-persistent
odbc.check_persistent = {{#odbc_check_persistent}}On{{/odbc_check_persistent}}{{^odbc_check_persistent}}Off{{/odbc_check_persistent}}

; http://php.net/odbc.max-persistent
odbc.max_persistent = {{#odbc_max_persistent}}{{odbc_max_persistent}}{{/odbc_max_persistent}}{{^odbc_max_persistent}}-1{{/odbc_max_persistent}}

; http://php.net/odbc.max-links
odbc.max_links = {{#odbc_max_links}}{{odbc_max_links}}{{/odbc_max_links}}{{^odbc_max_links}}-1{{/odbc_max_links}}

; http://php.net/odbc.defaultlrl
odbc.defaultlrl = {{#odbc_defaultlrl}}{{odbc_defaultlrl}}{{/odbc_defaultlrl}}{{^odbc_defaultlrl}4096{{/odbc_defaultlrl}}

; http://php.net/odbc.defaultbinmode
odbc.defaultbinmode = {{#odbc_defaultbinmode}}{{odbc_defaultbinmode}}{{/odbc_defaultbinmode}}{{^odbc_defaultbinmode}}1{{/odbc_defaultbinmode}}

; Not exactly sure what this is...
;birdstep.max_links = -1

[Interbase]
; Allow or prevent persistent links.
ibase.allow_persistent = 1

; Maximum number of persistent links.  -1 means no limit.
ibase.max_persistent = -1

; Maximum number of links (persistent + non-persistent).  -1 means no limit.
ibase.max_links = -1

; Default timestamp format.
ibase.timestampformat = "%Y-%m-%d %H:%M:%S"

; Default date format.
ibase.dateformat = "%Y-%m-%d"

; Default time format.
ibase.timeformat = "%H:%M:%S"

[MySQL]
; http://php.net/mysql.allow_local_infile
mysql.allow_local_infile = {{#mysql_allow_local_infile}}On{{/mysql_allow_local_infile}}{{^mysql_allow_local_infile}}Off{{/mysql_allow_local_infile}}

; http://php.net/mysql.allow-persistent
mysql.allow_persistent = {{#mysql_allow_persistent}}On{{/mysql_allow_persistent}}{{^mysql_allow_persistent}}Off{{/mysql_allow_persistent}}

; http://php.net/mysql.cache_size
mysql.cache_size = 2000

; http://php.net/mysql.max-persistent
mysql.max_persistent = -1

; http://php.net/mysql.max-links
mysql.max_links = -1

; http://php.net/mysql.connect-timeout
mysql.connect_timeout = 60

; http://php.net/mysql.trace-mode
mysql.trace_mode = {{#mysql_trace_mode}}On{{/mysql_trace_mode}}{{^mysql_trace_mode}}Off{{/mysql_trace_mode}}

[MySQLi]

; http://php.net/mysqli.max-persistent
mysqli.max_persistent = -1

; http://php.net/mysqli.max-links
mysqli.max_links = -1

; http://php.net/mysqli.cache_size
mysqli.cache_size = 2000

; Allow or prevent reconnect
mysqli.reconnect = {{#mysqli_reconnect}}On{{/mysqli_reconnect}}{{^mysqli_reconnect}}Off{{/mysqli_reconnect}}

[mysqlnd]
; http://php.net/mysqlnd.collect_statistics
mysqlnd.collect_statistics = {{#mysqlnd_collect_statistics}}On{{/mysqlnd_collect_statistics}}{{^mysqlnd_collect_statistics}}Off{{/mysqlnd_collect_statistics}}

; http://php.net/mysqlnd.collect_memory_statistics
mysqlnd.collect_memory_statistics = {{#mysqlnd_collect_memory_statistics}}On{{/mysqlnd_collect_memory_statistics}}{{^mysqlnd_collect_memory_statistics}}Off{{/mysqlnd_collect_memory_statistics}}

[PostgresSQL]
; http://php.net/pgsql.allow-persistent
pgsql.allow_persistent = {{#pgsql_allow_persistent}}On{{/pgsql_allow_persistent}}{{^pgsql_allow_persistent}}Off{{/pgsql_allow_persistent}}

; http://php.net/pgsql.auto-reset-persistent
pgsql.auto_reset_persistent = {{#pgsql_auto_reset_persistent}}On{{/pgsql_auto_reset_persistent}}{{^pgsql_auto_reset_persistent}}Off{{/pgsql_auto_reset_persistent}}

; http://php.net/pgsql.max-persistent
pgsql.max_persistent = -1

; http://php.net/pgsql.max-links
pgsql.max_links = -1

; http://php.net/pgsql.ignore-notice
pgsql.ignore_notice = 0

; http://php.net/pgsql.log-notice
pgsql.log_notice = 0

[Sybase-CT]
; http://php.net/sybct.allow-persistent
sybct.allow_persistent = {{#sybct_allow_persistent}}On{{/sybct_allow_persistent}}{{^sybct_allow_persistent}}Off{{/sybct_allow_persistent}}

; http://php.net/sybct.max-persistent
sybct.max_persistent = -1

; http://php.net/sybct.max-links
sybct.max_links = -1

; http://php.net/sybct.min-server-severity
sybct.min_server_severity = 10

; http://php.net/sybct.min-client-severity
sybct.min_client_severity = 10

[bcmath]
; http://php.net/bcmath.scale
bcmath.scale = 0

[Session]
; http://php.net/session.save-handler
session.save_handler = files

; Whether to use cookies.
; http://php.net/session.use-cookies
session.use_cookies = 1

; http://php.net/session.use-only-cookies
session.use_only_cookies = 1

; http://php.net/session.name
session.name = PHPSESSID

; http://php.net/session.auto-start
session.auto_start = 0

; http://php.net/session.cookie-lifetime
session.cookie_lifetime = 0

; http://php.net/session.cookie-path
session.cookie_path = /

; http://php.net/session.serialize-handler
session.serialize_handler = php

; http://php.net/session.gc-probability
session.gc_probability = 1

; http://php.net/session.gc-divisor
session.gc_divisor = 1000

; http://php.net/session.gc-maxlifetime
session.gc_maxlifetime = 1440

; http://php.net/session.bug-compat-42
session.bug_compat_42 = {{#session_bug_compat_42}}On{{/session_bug_compat_42}}{{^session_bug_compat_42}}Off{{/session_bug_compat_42}}

; http://php.net/session.bug-compat-warn
session.bug_compat_warn = {{#session_bug_compat_warn}}On{{/session_bug_compat_warn}}{{^session_bug_compat_warn}}Off{{/session_bug_compat_warn}}

; http://php.net/session.entropy-length
session.entropy_length = 0

; http://php.net/session.cache-limiter
session.cache_limiter = nocache

; http://php.net/session.cache-expire
session.cache_expire = 180

; http://php.net/session.use-trans-sid
session.use_trans_sid = 0

; http://php.net/session.hash-function
session.hash_function = 0

; http://php.net/session.hash-bits-per-character
session.hash_bits_per_character = 5

; http://php.net/url-rewriter.tags
url_rewriter.tags = "a=href,area=href,frame=src,input=src,form=fakeentry"

[MSSQL]
; Allow or prevent persistent links.
mssql.allow_persistent = {{#mssql_allow_persistent}}On{{/mssql_allow_persistent}}{{^mssql_allow_persistent}}Off{{/mssql_allow_persistent}}

; Maximum number of persistent links.  -1 means no limit.
mssql.max_persistent = -1

; Maximum number of links (persistent+non persistent).  -1 means no limit.
mssql.max_links = -1

; Minimum error severity to display.
mssql.min_error_severity = 10

; Minimum message severity to display.
mssql.min_message_severity = 10

; Compatibility mode with old versions of PHP 3.0.
mssql.compatability_mode = {{#mssql_compatability_mod}}On{{/mssql_compatability_mod}}{{^mssql_compatability_mod}}Off{{/mssql_compatability_mod}}

; Use NT authentication when connecting to the server
mssql.secure_connection = {{#mssql_secure_connection}}On{{/mssql_secure_connection}}{{^mssql_secure_connection}}Off{{/mssql_secure_connection}}

[Tidy]
; http://php.net/tidy.clean-output
tidy.clean_output = {{#tidy_clean_output}}On{{/tidy_clean_output}}{{^tidy_clean_output}}Off{{/tidy_clean_output}}

[soap]
; http://php.net/soap.wsdl-cache-enabled
soap.wsdl_cache_enabled=1

; http://php.net/soap.wsdl-cache-dir
soap.wsdl_cache_dir="/tmp"

; http://php.net/soap.wsdl-cache-ttl
soap.wsdl_cache_ttl=86400

; Sets the size of the cache limit. (Max. number of WSDL files to cache)
soap.wsdl_cache_limit = 5

[ldap]
; Sets the maximum number of open links or -1 for unlimited.
ldap.max_links = -1

[cgi]
cgi.fix_pathinfo = 0

